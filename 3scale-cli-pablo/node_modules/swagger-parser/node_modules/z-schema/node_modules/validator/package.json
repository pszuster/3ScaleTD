    "pretest": "npm run lint && npm run build",
    "test": "mocha --reporter spec",
    "coveralls": "istanbul cover _mocha --report lcovonly -x validator.js -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
